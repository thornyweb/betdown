{
  "name": "bet-down",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.7",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-modal": "^3.5.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "reactstrap": "^6.3.1"
  },
  "scripts": {
    "watch-js": "react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "test-js": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css watch-js",
    "build": "npm run build-css && react-scripts-ts build"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bootstrap": "^4.1.2",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/mongoose": "^5.2.5",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/reactstrap": "^6.0.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "typescript": "^3.0.1"
  }
}
